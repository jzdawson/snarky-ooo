{
  "redirects": [
    { "source": "/", "destination": "/snarkyOOO", "permanent": false }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/snarkyOOO/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/snarkyOOO/(.*)", "destination": "/index.html" },
    { "source": "/snarkyOOO", "destination": "/index.html" }
  ]
}
